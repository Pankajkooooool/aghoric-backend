{
    "manifest_version": 3,
    "name": "E-commerce Product Lister",
    "version": "1.0",
    "description": "A Chrome extension to list products on e-commerce platforms.",
    "permissions": ["activeTab", "storage"],
    "host_permissions": ["http:localhost:3000/"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }
  